<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:rowCount="6"
    android:columnCount="6"
    tools:context="com.example.intern.toolbox.MainActivity">

    <Button
        android:id="@+id/compassButton"
        android:layout_width="130dp"
        android:layout_height="100dp"
        android:layout_column="0"
        android:layout_row="0"
        android:text="" />

    <Button
        android:id="@+id/levelerButton"
        android:layout_width="130dp"
        android:layout_height="100dp"
        android:layout_column="2"
        android:layout_row="0"/>

    <Button
        android:id="@+id/magnetButton"
        android:layout_width="130dp"
        android:layout_height="100dp"
        android:layout_column="1"
        android:layout_row="0"
        android:text="" />

    <Button
        android:id="@+id/vibrButton"
        android:layout_width="130dp"
        android:layout_height="100dp"
        android:layout_column="0"
        android:layout_row="2"
        android:text="" />

    <Button
        android:id="@+id/flashButton"
        android:layout_width="130dp"
        android:layout_height="100dp"
        android:layout_column="1"
        android:layout_row="2"
        android:text="" />

    <Button
        android:id="@+id/uConvButton"
        android:layout_width="130dp"
        android:layout_height="100dp"
        android:layout_column="2"
        android:layout_row="2"
        android:text="" />

    <Button
        android:id="@+id/counterButton"
        android:layout_width="130dp"
        android:layout_height="100dp"
        android:layout_column="1"
        android:layout_row="4"
        android:text="" />

    <Button
        android:id="@+id/stpWatchButton"
        android:layout_width="130dp"
        android:layout_height="100dp"
        android:layout_column="2"
        android:layout_row="4"
        android:text="" />

    <Button
        android:id="@+id/currButton"
        android:layout_width="130dp"
        android:layout_height="100dp"
        android:layout_column="0"
        android:layout_row="4"
        android:text="" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="0"
        android:layout_row="3"
        android:textAlignment="center"
        android:text="Vibrator" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="0"
        android:layout_row="1"
        android:text="Compass" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="1"
        android:layout_row="1"
        android:text="Magnito Meter" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="2"
        android:layout_row="1"
        android:text="Leveler" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="1"
        android:layout_row="3"
        android:text="Flash" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="2"
        android:layout_row="3"
        android:text="Unit Converter" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="0"
        android:layout_row="5"
        android:text="Currency" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="1"
        android:layout_row="5"
        android:text="Counter" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="2"
        android:layout_row="5"
        android:text="Stop-watch" />
</GridLayout>
